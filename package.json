  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@types/node": "^16.11.25",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.7",
    "consola": "^2.15.3",
    "element-plus": "^2.2.0",
    "nanoid": "^3.3.4",
    "postcss": "^8.4.13",
    "tailwindcss": "^3.0.24",
    "typescript": "~4.5.5",
